{
  "name": "cndr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r --filter='!@cndr/server' run build",
    "build:all": "pnpm -r run build",
    "dev": "pnpm --filter @cndr/server run dev",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "type-check": "tsc -b",
    "test": "pnpm -r run test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.27.11",
    "@types/node": "22.10.6",
    "@vitest/coverage-v8": "2.1.8",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vitest": "2.1.8"
  }
}
